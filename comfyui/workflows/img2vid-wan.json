{
  "_meta": {
    "title": "Wan 2.2 Image to Video",
    "description": "Image-to-video using Wan 2.2 5B. Strong motion quality.",
    "optimal_settings": {
      "resolution": "832x480 or 480x832",
      "frames": "81 (recommended)",
      "cfg": "5.0",
      "steps": "30",
      "fps": "16"
    },
    "requirements": ["Wan 2.2 5B pack files"]
  },
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "UNETLoader",
    "inputs": {
      "unet_name": "wan2.2_ti2v_5B_fp16.safetensors",
      "weight_dtype": "default"
    }
  },
  "3": {
    "class_type": "DualCLIPLoader",
    "inputs": {
      "clip_name1": "umt5_xxl_encoder_q4_k_m.gguf",
      "clip_name2": "open_clip_vit_h_14_text_encoder.safetensors",
      "type": "wan"
    }
  },
  "4": {
    "class_type": "VAELoader",
    "inputs": {
      "vae_name": "wan_2.2_vae.safetensors"
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}, cinematic motion, high quality, smooth movement",
      "clip": ["3", 0]
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "blurry, low quality, distorted, static, watermark",
      "clip": ["3", 0]
    }
  },
  "7": {
    "class_type": "ImageScaleToTotalPixels",
    "inputs": {
      "image": ["1", 0],
      "upscale_method": "lanczos",
      "megapixels": 0.4
    }
  },
  "8": {
    "class_type": "VAEEncode",
    "inputs": {
      "pixels": ["7", 0],
      "vae": ["4", 0]
    }
  },
  "9": {
    "class_type": "EmptyLatentVideo",
    "inputs": {
      "width": 832,
      "height": 480,
      "length": "{{frames}}",
      "batch_size": 1
    }
  },
  "10": {
    "class_type": "KSampler",
    "inputs": {
      "seed": "{{seed}}",
      "steps": 30,
      "cfg": 5.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["2", 0],
      "positive": ["5", 0],
      "negative": ["6", 0],
      "latent_image": ["9", 0]
    }
  },
  "11": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["10", 0],
      "vae": ["4", 0]
    }
  },
  "12": {
    "class_type": "SaveAnimatedWEBP",
    "inputs": {
      "images": ["11", 0],
      "fps": "{{fps}}",
      "lossless": false,
      "quality": 85,
      "method": "default",
      "filename_prefix": "wan_video"
    }
  }
}
