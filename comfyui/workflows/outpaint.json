{
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "ImagePadForOutpaint",
    "inputs": {
      "image": ["1", 0],
      "left": "{{extend_left}}",
      "top": "{{extend_top}}",
      "right": "{{extend_right}}",
      "bottom": "{{extend_bottom}}",
      "feathering": 40
    }
  },
  "3": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0_inpainting_0.1.safetensors"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}",
      "clip": ["3", 1]
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{negative_prompt}}",
      "clip": ["3", 1]
    }
  },
  "6": {
    "class_type": "VAEEncode",
    "inputs": {
      "pixels": ["2", 0],
      "vae": ["3", 2]
    }
  },
  "7": {
    "class_type": "SetLatentNoiseMask",
    "inputs": {
      "samples": ["6", 0],
      "mask": ["2", 1]
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["3", 0],
      "positive": ["4", 0],
      "negative": ["5", 0],
      "latent_image": ["7", 0],
      "seed": "{{seed}}",
      "steps": 25,
      "cfg": 7.0,
      "sampler_name": "euler_ancestral",
      "scheduler": "normal",
      "denoise": 1.0
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["3", 2]
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["9", 0],
      "filename_prefix": "{{filename_prefix}}"
    }
  }
}
