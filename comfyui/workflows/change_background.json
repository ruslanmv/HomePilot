{
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "ImageRemoveBackgroundRembg",
    "inputs": {
      "image": ["1", 0]
    }
  },
  "3": {
    "class_type": "ImageToMask",
    "inputs": {
      "image": ["2", 0],
      "channel": "alpha"
    }
  },
  "4": {
    "class_type": "InvertMask",
    "inputs": {
      "mask": ["3", 0]
    }
  },
  "5": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "{{ckpt_name}}"
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}",
      "clip": ["5", 1]
    }
  },
  "7": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{negative_prompt}}",
      "clip": ["5", 1]
    }
  },
  "8": {
    "class_type": "VAEEncode",
    "inputs": {
      "pixels": ["1", 0],
      "vae": ["5", 2]
    }
  },
  "9": {
    "class_type": "SetLatentNoiseMask",
    "inputs": {
      "samples": ["8", 0],
      "mask": ["4", 0]
    }
  },
  "10": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["5", 0],
      "positive": ["6", 0],
      "negative": ["7", 0],
      "latent_image": ["9", 0],
      "seed": "{{seed}}",
      "steps": 25,
      "cfg": 7.0,
      "sampler_name": "euler_ancestral",
      "scheduler": "normal",
      "denoise": 1.0
    }
  },
  "11": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["10", 0],
      "vae": ["5", 2]
    }
  },
  "12": {
    "class_type": "ImageCompositeMasked",
    "inputs": {
      "destination": ["11", 0],
      "source": ["2", 0],
      "mask": ["3", 0],
      "x": 0,
      "y": 0,
      "resize_source": false
    }
  },
  "13": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["12", 0],
      "filename_prefix": "{{filename_prefix}}"
    }
  }
}
