{
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "UpscaleModelLoader",
    "inputs": {
      "model_name": "{{model_name}}"
    }
  },
  "3": {
    "class_type": "ImageUpscaleWithModel",
    "inputs": {
      "upscale_model": ["2", 0],
      "image": ["1", 0]
    }
  },
  "4": {
    "class_type": "ImageScale",
    "inputs": {
      "image": ["3", 0],
      "width": "{{width}}",
      "height": "{{height}}",
      "upscale_method": "lanczos",
      "crop": "disabled"
    }
  },
  "5": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["4", 0],
      "filename_prefix": "{{filename_prefix}}"
    }
  }
}
