{
  "_meta": {
    "title": "HunyuanVideo T2V/I2V",
    "description": "Video generation using HunyuanVideo GGUF. Requires ComfyUI-GGUF nodes.",
    "optimal_settings": {
      "resolution": "848x480 (720p scaled)",
      "frames": "45-129",
      "cfg": "6.0",
      "steps": "30",
      "fps": "24"
    },
    "requirements": ["ComfyUI-GGUF"]
  },
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "UnetLoaderGGUF",
    "inputs": {
      "unet_name": "hunyuanvideo-q4_k_m.gguf"
    }
  },
  "3": {
    "class_type": "DualCLIPLoaderGGUF",
    "inputs": {
      "clip_name1": "llava_llama3_fp8_scaled.safetensors",
      "clip_name2": "clip_l.safetensors",
      "type": "hunyuan_video"
    }
  },
  "4": {
    "class_type": "VAELoader",
    "inputs": {
      "vae_name": "hunyuan_video_vae_bf16.safetensors"
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}, cinematic, high quality video, smooth motion",
      "clip": ["3", 0]
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "blurry, low quality, distorted, watermark, static",
      "clip": ["3", 0]
    }
  },
  "7": {
    "class_type": "ImageScaleToTotalPixels",
    "inputs": {
      "image": ["1", 0],
      "upscale_method": "lanczos",
      "megapixels": 0.4,
      "resolution_steps": 64
    }
  },
  "8": {
    "class_type": "VAEEncode",
    "inputs": {
      "pixels": ["7", 0],
      "vae": ["4", 0]
    }
  },
  "9": {
    "class_type": "EmptyHunyuanLatentVideo",
    "inputs": {
      "width": 848,
      "height": 480,
      "length": "{{frames}}",
      "batch_size": 1
    }
  },
  "10": {
    "class_type": "KSampler",
    "inputs": {
      "seed": "{{seed}}",
      "steps": "{{steps}}",
      "cfg": "{{cfg}}",
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": "{{denoise}}",
      "model": ["2", 0],
      "positive": ["5", 0],
      "negative": ["6", 0],
      "latent_image": ["9", 0]
    }
  },
  "11": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["10", 0],
      "vae": ["4", 0]
    }
  },
  "12": {
    "class_type": "SaveAnimatedWEBP",
    "inputs": {
      "images": ["11", 0],
      "fps": "{{fps}}",
      "lossless": false,
      "quality": 85,
      "method": "default",
      "filename_prefix": "hunyuan_video"
    }
  }
}
