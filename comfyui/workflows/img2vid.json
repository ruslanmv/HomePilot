{
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{image_path}}"
    }
  },
  "2": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "svd.safetensors"
    }
  },
  "3": {
    "class_type": "VAEEncode",
    "inputs": {
      "pixels": ["1", 0],
      "vae": ["2", 2]
    }
  },
  "4": {
    "class_type": "VideoLinearCFGGuidance",
    "inputs": {
      "model": ["2", 0],
      "min_cfg": 1.0
    }
  },
  "5": {
    "class_type": "SVD_img2vid_Conditioning",
    "inputs": {
      "clip_vision": ["2", 1],
      "init_image": ["1", 0],
      "vae": ["2", 2],
      "width": 1024,
      "height": 576,
      "video_frames": 25,
      "motion_bucket_id": 127,
      "fps": 6,
      "augmentation_level": 0.0
    }
  },
  "6": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 42,
      "steps": 20,
      "cfg": 2.5,
      "sampler_name": "euler",
      "scheduler": "karras",
      "denoise": 1.0,
      "model": ["4", 0],
      "positive": ["5", 0],
      "negative": ["5", 1],
      "latent_image": ["5", 2]
    }
  },
  "7": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["6", 0],
      "vae": ["2", 2]
    }
  },
  "8": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "img2vid_frames",
      "images": ["7", 0]
    }
  }
}
