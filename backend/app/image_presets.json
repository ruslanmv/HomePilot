        "cfg": 7.0
      },
      "model_overrides": {
        "sd15":            { "steps": 20, "cfg": 7.0 },
        "sdxl":            { "steps": 25, "cfg": 5.0 },
        "pony_xl":         { "steps": 15, "cfg": 5.0 },
