  "model_rules": {
    "sd15": {
      "default_aspect_ratio": "1:1",
      "architecture": "sd15",
      "notes": "SD 1.5 models work best at 512px base resolution to avoid duplication artifacts"
    },
    "sdxl": {
      "default_aspect_ratio": "1:1",
      "architecture": "sdxl",
      "notes": "SDXL models native at 1024px base resolution"
    },
    "pony_xl": {
      "default_aspect_ratio": "2:3",
      "architecture": "pony_xl",
      "clip_skip": 2,
      "scheduler": "karras",
      "notes": "Pony Diffusion V6 XL — SDXL finetune. MUST use clip_skip=2. Uses score_9/score_8_up quality system. Trained on Danbooru/e621 booru tags."
    },
    "noobai_xl": {
      "default_aspect_ratio": "2:3",
      "architecture": "noobai_xl",
      "clip_skip": 2,
      "scheduler": "karras",
      "notes": "NoobAI-XL EPS-prediction. Standard SDXL workflow. Uses 'very awa' aesthetic tag + Danbooru tags. Best character/artist knowledge."
    },
    "noobai_xl_vpred": {
      "default_aspect_ratio": "2:3",
      "architecture": "noobai_xl_vpred",
      "clip_skip": 2,
      "scheduler": "normal",
      "prediction_type": "v_prediction",
      "notes": "NoobAI-XL V-Prediction. Requires v-pred scheduler support (ComfyUI/Forge/reForge). Better color but NOT compatible with standard A1111."
    },
    "flux_schnell": {
      "default_aspect_ratio": "1:1",
      "architecture": "flux_schnell",
      "notes": "Flux Schnell - fast 4-step turbo model"
    },
    "flux_dev": {
      "default_aspect_ratio": "1:1",
      "architecture": "flux_dev",
      "notes": "Flux Dev - high quality model"
    }
  }
